{
  "name": "@the-platform/web",
  "version": "0.1.0",
  "packageManager": "yarn@3.2.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dbup": "dotenv -e .env.local -- prisma db push",
    "dbg": "dotenv -e .env.local -- prisma generate",
    "load": "dotenv -e .env.local -- ts-node --compiler-options {\"module\":\"CommonJS\"} ./prisma/seed.ts",
    "seed": "dotenv -e .env.local -- prisma db seed",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "vercel-build": "prisma generate && prisma migrate deploy && next build"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.11.0",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@hookform/devtools": "^4.2.2",
    "@hookform/resolvers": "^2.9.6",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "latest",
    "@tanstack/react-table": "^8.2.6",
    "@trpc/client": "^9.25.3",
    "@trpc/next": "^9.25.3",
    "@trpc/react": "^9.25.3",
    "@trpc/server": "^9.25.3",
    "class-validator": ">=0.12.0",
    "classnames": "^2.3.1",
    "js-yaml": "^4.1.0",
    "next": "12.2.2",
    "next-auth": "^4.9.0",
    "nodemailer": "^6.7.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.33.1",
    "react-query": "^3.39.1",
    "recharts": "^2.1.12",
    "superjson": "^1.9.1",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.2",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.0.6",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.7",
    "dotenv-cli": "^6.0.0",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.2",
    "postcss": "^8.4.14",
    "prisma": "latest",
    "tailwindcss": "^3.1.5",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
